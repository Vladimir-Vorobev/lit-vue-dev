(loadingStatus) {\n    loadingStatus = \"mounted\"\n    console.log(loadingStatus);\n  },\n  beforeUpdate(loadingStatus) {\n    loadingStatus = \"beforeUpdate\"\n    console.log(loadingStatus);\n  },\n  updated(loadingStatus) {\n    loadingStatus = \"updated\"\n    console.log(loadingStatus);\n  },\n  beforeDestroy(loadingStatus) {\n    loadingStatus = \"beforeDestroy\"\n    console.log(loadingStatus);\n  },\n  destroyed(loadingStatus) {\n    loadingStatus = \"destroyed\"\n    console.log(loadingStatus);\n  },\n}\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\n.main {\ndisplay: flex;\njustify-content: center;\n}\n.button{\ndisplay: flex;\ncursor: pointer;\njustify-content: center;\nwidth: 150px;\nheight: 50px;\nmargin-left: auto;\nmargin-right: auto;\nfont-weight: 700;\nfont-size: 15px;\ncolor: black;\nborder-radius: 25px;\nborder-color: black;\nborder-style: solid;\ntext-align: center;\nline-height: 50px;\n}\n.counter{\n  margin-top: 10px\n}\n</style>\n"],"sourceRoot":"src/components"}]}